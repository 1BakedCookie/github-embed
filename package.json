{
  "name": "github-embed",
  "description": "Github emvedder",
  "main": "server.js",
  "scripts": {
    "unit": "tape -r babel-register -r babel-polyfill test/**/*Spec.js",
    "prepublish": "rimraf npm && npm run bundle-npm-css && babel src -d npm",
    "bundle-npm-css": "webpack --config webpack-only-css.config.js",
    "start": "node server.js",
    "bundle": "webpack",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "commit": "git-cz"
  },
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/finom/github-embed.git",
      "branch": "gh-pages",
      "folder": "bundle",
      "script": "npm run bundle",
      "commit": "$npm_package_version",
      "user": {
        "email": "andrey.a.gubanov@gmail.com",
        "name": "Andrey Gubanov (his digital copy)"
      }
    },
    "commitizen": {
      "path": "cz-simple-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finom/github-embed.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/github-embed/issues"
  },
  "homepage": "https://github.com/finom/github-embed#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "balajs": "^0.1.9",
    "classnames": "^2.2.5",
    "github-url-parse": "^0.1.0",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-loader": "^2.4.0",
    "react-redux": "^4.1.0",
    "redux": "^3.1.1",
    "redux-thunk": "^2.1.0",
    "highlight.js": "^9.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015-loose": "^6.1.3",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.25.0",
    "cz-simple-conventional-changelog": "0.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "githook": "0.0.1",
    "node-libs-browser": "^0.5.2",
    "postcss-calc": "^5.3.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-nested": "^1.0.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "rimraf": "^2.5.4",
    "semantic-release": "^4.3.5",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
