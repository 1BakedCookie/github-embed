{
  "name": "github-embed",
  "description": "Github embedder",
  "version": "0.0.0-auto",
  "main": "npm/index.js",
  "scripts": {
    "test": "npm run lint",
    "start": "node server.js",
    "lint": "eslint ./src",
    "bundle": "webpack && shx mkdir -p npm/css/ && shx cp -r bundle/github-embed.css npm/css/github-embed.css",
    "deploy-to-git": "deploy-to-git",
    "semantic-release": "semantic-release pre && shx rm -rf npm bundle && npm run deploy-to-git && babel src -d npm && npm publish && semantic-release post",
    "commit": "git-cz",
    "check-updates": "ncu",
    "upgrade": "npm run check-updates -- --upgradeAll && npm install"
  },
  "config": {
    "deployToGit": {
      "repository": "https://$GH_TOKEN@github.com/finom/github-embed.git",
      "branch": "gh-pages",
      "folder": "bundle",
      "script": "npm run bundle",
      "commit": "$npm_package_version",
      "user": {
        "email": "andrey.a.gubanov@gmail.com",
        "name": "Andrey Gubanov (his digital copy)"
      }
    },
    "commitizen": {
      "path": "cz-simple-conventional-changelog"
    },
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert"
      ]
    },
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/finom/github-embed.git"
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/finom/github-embed/issues"
  },
  "homepage": "https://github.com/finom/github-embed#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "balajs": "^0.1.9",
    "classnames": "^2.2.5",
    "github-url-parse": "^0.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-hot-loader": "^3.0.0-beta.3",
    "react-loader": "^2.4.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "highlight.js": "^9.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "commitizen": "^2.8.6",
    "css-loader": "^0.25.0",
    "cz-simple-conventional-changelog": "0.0.1",
    "deploy-to-git": "0.0.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^11.0.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.1",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "ghooks": "^1.3.2",
    "ncp": "^2.0.0",
    "node-libs-browser": "^1.0.0",
    "npm-check-updates": "^2.8.0",
    "postcss-calc": "^5.3.1",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-nested": "^1.0.0",
    "semantic-release": "^4.3.5",
    "shx": "^0.1.4",
    "style-loader": "^0.13.1",
    "validate-commit-msg": "^2.8.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1"
  }
}
